<tool id="addCoverageToDataset" name="addCoverageToDataset" version="0.1.0+galaxy0" python_template_version="3.5" profile="21.05">
    <requirements>
        <container type="docker">genomehubs/blobtoolkit:latest</container>
    </requirements>
    <command detect_errors="exit_code"><![CDATA[
        blobtools add --cov $cov
        --threads $threads
        --replace $replace
        ~\Blobdir
    ]]></command>
    <inputs>
        <param name="cov" label="coverage" type="data"/>
        <param name="threads" label="number of threads" default_value="1" type="integer" optional="true"/>
        <param name="replace" type="boolean" default_value="false" optional="true"/>
    </inputs>
    <outputs>
    </outputs>
    <help><![CDATA[
        TODO: Fill in help.
    ]]></help>
</tool>