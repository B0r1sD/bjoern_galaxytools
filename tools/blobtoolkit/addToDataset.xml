<tool id="addToDataset" name="addToDataset" version="0.1.0+galaxy0" python_template_version="3.5" profile="21.05">
    <requirements>
        <container type="docker">https://github.com/blobtoolkit/blobtoolkit-docker</container>
    </requirements>
    <command detect_errors="exit_code"><![CDATA[
        <!-- adding hits -->
        blobtools add \
        --hits ~/BTK_TUTORIAL/FILES/ASSEMBLY_NAME.ncbi.blastn.out \
        --hits ~/BTK_TUTORIAL/FILES/ASSEMBLY_NAME.diamond.blastx.out \
        --taxrule bestsumorder \
        --taxdump ~/blobtoolkit/taxdump \
        ~/BTK_TUTORIAL/DATASETS/ASSEMBLY_NAME
    ]]></command>
    <inputs>
        <param name="blast" type="data" format="ncbi.blastn.out" label="ASSEMBLY_NAME.ncbi.blastn.out"/>
        <param name="diamond blast" type="data" format="diamond.blastx.out" label="ASSEMBLY_NAME.diamond.blastx.out"/>
    </inputs>
    <outputs>
    </outputs>
    <help><![CDATA[
        TODO: Fill in help.
    ]]></help>
</tool>