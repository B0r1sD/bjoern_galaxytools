<tool id="addTextfilesToDataset" name="addTextfilesToDataset" version="0.1.0+galaxy0" python_template_version="3.5" profile="21.05">
    <requirements>
        <container type="docker">genomehubs/blobtoolkit:latest</container>
    </requirements>
    <command detect_errors="exit_code"><![CDATA[
        blobtools add
        --text $text
        --delimiter $delimiter
        --text-cols $cols
        --text-header $header
        --text-no-array $noarray
        --replace $replace
    ]]></command>
    <inputs>
        <param name="text" type="data" format="csv" />
        <param name="delimiter" type="text" default_value=" " optional="true"/>
        <param name="cols" type="text"/>
        <param name="header" type="boolean" default_value="false" optional="true"/>
        <param name="noarray" type="boolean" default_value="false" optional="true"/>
        <param name="replace" type="boolean" default_value="false" optional="true"/>
    </inputs>
    <outputs>
    </outputs>
    <help><![CDATA[
        TODO: Fill in help.
    ]]></help>
</tool>